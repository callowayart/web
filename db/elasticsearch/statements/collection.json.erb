{
    "size" : 1000,

    "query": { "bool": {
      <% tags.each_with_index do | tag, index | %>
        "must": { "term": {  "__tags" : "<%= tag %>"  }},
      <% end %>

      "must_not": { "term": { "__tags" : "backendonly" }},
      "must_not": { "term": { "__tags" : "not-available" }}

    }},

    "sort": [
      { "__title": "asc" }
    ]
    
}